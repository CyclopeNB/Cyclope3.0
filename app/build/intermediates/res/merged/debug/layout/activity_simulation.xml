<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.example.ly34u.cyclope10.PiloterActivity">

    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/mjpegview"
        android:layout_width="585dp"
        android:layout_height="450dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" />

    <io.github.controlwear.virtual.joystick.android.JoystickView xmlns:custom="http://schemas.android.com/apk/res-auto"
        android:id="@+id/joystickView"
        android:layout_width="302dp"
        android:layout_height="302dp"
        android:background="@drawable/cross3"
        custom:JV_backgroundSizeRatio="66%"
        custom:JV_borderColor="#50FFFFFF"
        custom:JV_borderWidth="4dp"
        custom:JV_buttonImage="@drawable/stick8"
        custom:JV_buttonSizeRatio="25%"
        custom:JV_fixedCenter="false"
        custom:layout_constraintLeft_toLeftOf="parent"
        custom:layout_constraintTop_toTopOf="parent"
        custom:layout_constraintBottom_toBottomOf="parent"
        custom:layout_constraintRight_toRightOf="parent"
        custom:layout_constraintHorizontal_bias="0.0"
        custom:layout_constraintVertical_bias="1.0" />

    <io.github.controlwear.virtual.joystick.android.JoystickView xmlns:custom="http://schemas.android.com/apk/res-auto"
        android:id="@+id/joystickView2"
        android:layout_width="303dp"
        android:layout_height="303dp"
        android:background="@drawable/cross3"
        custom:JV_backgroundSizeRatio="66%"
        custom:JV_borderColor="#50FFFFFF"
        custom:JV_borderWidth="4dp"
        custom:JV_buttonImage="@drawable/stick8"
        custom:JV_buttonSizeRatio="25%"
        custom:JV_fixedCenter="false"
        custom:layout_constraintLeft_toLeftOf="parent"
        custom:layout_constraintTop_toTopOf="parent"
        custom:layout_constraintBottom_toBottomOf="parent"
        custom:layout_constraintRight_toRightOf="parent"
        custom:layout_constraintHorizontal_bias="0.998"
        custom:layout_constraintVertical_bias="0.994" />


</android.support.constraint.ConstraintLayout>
